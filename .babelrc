{
    "presets": [
      "module:metro-react-native-babel-preset"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./src"],
          "extensions": [
            ".ios.ts",
            ".android.ts",
            ".ts",
            ".ios.tsx",
            ".android.tsx",
            ".jsx",
            ".js",
            ".json"
          ],
          "alias": {
            "@partials": "./src/partials",
            "@pages": "./src/components/pages",
            "@common": "./src/components/common",
            "@helpers": "./src/components/helpers",
            "@actions": "./src/store/actions"
            // "@assets": "./assets",
          }
        }
      ]
    ]
  }